    "@babel/preset-env": "^7.12.0",
    "acorn": "^8.0.4",
    "ajv": "^6.12.6",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "jest": "^26.5.3",
